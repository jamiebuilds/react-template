{
  "name": "wdym",
  "browserslist": [
    "> 5%",
    "not dead"
  ],
  "scripts": {
    "format": "prettier --write '**'",
    "format:check": "prettier --check '**'",
    "typecheck": "tsc --noEmit --pretty",
    "typecheck:watch": "tsc --noEmit --pretty --watch",
    "lint": "npm run -s lint:js && npm run -s lint:css",
    "lint:js": "eslint '**' --max-warnings=0 --format stylish",
    "lint:css": "stylelint '**'",
    "test": "NODE_ENV=production TS_NODE_FILES=true karma start --single-run --browsers ChromeHeadless karma.conf.ts",
    "test:watch": "NODE_ENV=production TS_NODE_FILES=true karma start --auto-watch --browsers ChromeHeadless karma.conf.ts",
    "build": "NODE_ENV=production webpack",
    "build:watch": "NODE_ENV=development webpack serve",
    "intl:extract": "formatjs extract 'src/**/*.tsx' --out-file '.temp/intl-messages.json' --format crowdin",
    "intl:compile": "formatjs compile '.temp/intl-messages.json' --out-file 'dist/intl/en.json' --format crowdin --ast",
    "start": "npm run -s build:watch",
    "prepublishOnly": "npm run -s build"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.10.4",
    "@emotion/babel-plugin": "^11.1.2",
    "@emotion/core": "^11.0.0",
    "@emotion/eslint-plugin": "^11.0.0",
    "@emotion/react": "^11.1.4",
    "@formatjs/cli": "^3.0.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/chai": "^4.2.11",
    "@types/karma": "^5.0.0",
    "@types/mocha": "^8.2.0",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/resize-observer-browser": "^0.1.5",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-intl": "^8.2.25",
    "chai": "^4.2.0",
    "css-loader": "^5.0.1",
    "date-fns": "^2.14.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-destructuring": "^2.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-formatjs": "^2.12.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-local-rules": "^1.0.1",
    "eslint-plugin-no-useless-const": "^1.0.1",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.0.6",
    "eslint-plugin-react-perf": "^3.3.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-tsdoc": "^0.2.11",
    "eslint-plugin-unicorn": "^27.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.8",
    "karma": "^6.0.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^5.0.0",
    "lint-staged": "^10.5.3",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intl": "^5.12.0",
    "react-refresh": "^0.9.0",
    "react-router-dom": "^5.2.0",
    "source-map-explorer": "^2.5.2",
    "style-loader": "^2.0.0",
    "stylelint": "^13.9.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-value-no-unknown-custom-properties": "^3.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.0",
    "webpack": "^5.20.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged | cat"
    }
  },
  "lint-staged": {
    "*.(md|ts|tsx|js|jsx|json)": [
      "prettier --write",
      "git add"
    ]
  }
}
